<% link = pagy_link_proc(pagy) %>
<div class="pagination-wrap">
    <ul class="pagination">
        <% if pagy.prev %>
            <li class="page-item">
                <%== link.call(pagy.prev, pagy_t('pagy.nav.prev')) %>
            </li>
        <% else  %>
            <li class="page-item disabled">
                <%== link.call(pagy.prev, pagy_t('pagy.nav.prev')) %>
            </li>
        <% end %>

        <% pagy.series.each do |item| %>
            <% if item.is_a?(Integer) %>
                <li id="testtest" class="page-item"><%== link.call(item) %></li>
            <% elsif item.is_a?(String) %>
                <li class="page-item active" aria-current="page"><%== link.call(item) %></li>
            <% elsif item == :gap %>
                <li class="page-item disabled gap"><a href="#" class="page-link"><%== pagy_t('pagy.nav.gap') %></a></li>
            <% end %>
        <% end %>

        <% if pagy.next %>
            <li class="page-item"><%== link.call(pagy.next, pagy_t('pagy.nav.next')) %></li>
        <% else %>
            <li class="page-item disabled"><%== link.call(pagy.next, pagy_t('pagy.nav.next')) %></li>
        <% end %>
    </ul>
</div>