<script>
  window.onload = function () {

    var chart = new CanvasJS.Chart("chartRegistration", {
      animationEnabled: true,
      title: {},
      axisY: {
        valueFormatString: "#0,,.",
        prefix: "0"
      },
      data: [{
        type: "splineArea",
        color: "rgba(8,173,168,1)",
        markerSize: 3,
        xValueFormatString: "MM",
        yValueFormatString: "0",
        dataPoints: [{
            x: new Date(2000, 0),
            y: 3289000
          },
          {
            x: new Date(2001, 0),
            y: 3830000
          },
          {
            x: new Date(2002, 0),
            y: 2009000
          },
          {
            x: new Date(2003, 0),
            y: 2840000
          },
          {
            x: new Date(2004, 0),
            y: 2396000
          },
          {
            x: new Date(2005, 0),
            y: 1613000
          },
        ]
      }]
    });
    chart.render();

  }
</script>

