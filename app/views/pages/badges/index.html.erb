<div class="d-flex main-wrapper">
  <%= render("components/sidebar") %>
  <div class="main-wrapper">
    <main class="main col-md-10 offset-md-2" role="main">
      <header class="header" role="banner">
        <%= render("components/dropdown_profile") %>
      </header>
      <div class="main-content">
          <div class="top">
            <div class="pagination-wrap">
              <ul class="pagination">
                <li class="page-item">
                  <a class="page-link" href="#" tabindex="-1" aria-disabled="true"><img
                    src="<%= asset_pack_path("views/assets/images/ic_arrow-left.svg") %>" alt="Previous"></a>
                </li>
                <li class="page-item"><a class="page-link" href="#">1</a></li>
                <li class="page-item active" aria-current="page">
                  <a class="page-link" href="#">2 <span class="sr-only">(current)</span></a>
                </li>
                <li class="page-item"><a class="page-link" href="#">3</a></li>
                <li class="page-item">
                  <a class="page-link" href="#"><img src="<%= asset_pack_path("views/assets/images/ic_arrow_right.svg") %>" alt="Next"></a>
                </li>
              </ul>
            </div>
            <div class="modal-wrap">
              <button type="button" class="btn btn-green" data-toggle="modal" data-target="#exampleModal">
                  Create Badge <img src="<%= asset_pack_path("views/assets/images/ic_create-admin.svg") %>" alt="add">
                </button>
                              
                <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                        <div class="modal-dialog" role="document">
                          <div class="modal-content">
                            <div class="modal-header">
                              <!-- <h5 class="modal-title" id="exampleModalLabel">Tambah Admin</h5> -->
                              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                              </button>
                            </div>
                            <div class="modal-body">
                              <h3>Tambah Badge</h3>
                              <div class="form-wrap">
                                  <label>Upload Image</label>
                                  <div class="form-group">
                                      <div class="upload-image">
                                        <div class="custom-button">
                                            <label class="upload-img" for="file-input">
                                              <img src="<%= asset_pack_path("views/assets/images/ic_img.svg") %>" alt="add">
                                            </label>
                                            <input type="file" name="pic" id="file-input" accept="image/*">
                                        </div>
                                              <span>
                                                      <p>Upload PNG/JPG</p>
                                                      <p>Maks 512x512</p>
                                              </span>
                                        </div>
                                    </div>
                                </div>
                              <div class="row">
                                  <div class="col-md-6 col-xs-12">
    
                                        <div class="form-wrap">
                                          <label>Nama Badge</label>
                                          <div class="form-group"> 
                                              <div class="form-group">
                                                  <input type="Cluster" class="form-control" placeholder="Nama Badge">
                                                </div>
                                            </div>
                                        </div>            
                                        <div class="form-wrap">
                                          <label>Deskripsi</label>
                                          <div class="form-group"> 
                                              <div class="form-group">
                                                  <textarea type="Email" class="form-control"></textarea>
                                                </div>
                                            </div>
                                        </div>

                                  </div>
                                  <div class="col-md-6 col-xs-12">
                                  </div>

                              </div>

                            </div>
                            <div class="modal-footer">
                              <button type="button" class="btn btn-solid-green" data-dismiss="modal">Tambah</button>
                              <button type="button" class="btn btn-transparent">Cancel</button>
                            </div>
                          </div>
                        </div>
                      </div>
            </div>
          </div>
