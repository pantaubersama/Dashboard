<div class="d-flex main-wrapper">
  <%= render("components/sidebar") %>
  <div class="main-wrapper">
    <main class="main col-md-10 offset-md-2" role="main">
      <%= render("components/header") %>
      <div class="main-content janji-politik">
        <div class="top">
          <div class="btn-back">
            <a href="#">
              <img src="<%= asset_pack_path("views/assets/images/ic_back.svg") %>"> Back
            </a>
          </div>
        </div>
        <div class="content">
                        <span class="display-desc">
                          <p>
                              <!-- Displaying Content 1 - 30 of 500 in total -->
                          </p>
                        </span>
          <div class="bg-white detail-profile">
            <div class="card">
              <div class="sub-profile">
                <span class="profile-image"><img src="<%= asset_pack_path("views/assets/images/profile.jpg") %>"></span>
                <span class="name">
                                    <h5>Nama   :  Aniah Muanjani</h5>
                                    <p>Cluster :  Partai Kuning Ceria</p>

                                </span>

              </div>
              <div class="right-desc">
                <p>Created at : 21/10/2023</p>
                <button class="btn btn-solid-green">EDIT</button>
              </div>
            </div>

            <div class="card-form">
              <div class="row">
                <div class="col-md-6">
                  <div class="form-wrap">
                    <label>Judul</label>
                    <div class="form-group">
                      <div class="form-group">
                                                <textarea type="Email" class="form-control" hresize
                                                          placeholder="@Yudho">

                                                </textarea>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="form-wrap">
                    <label>Deskripsi</label>
                    <div class="form-group">
                      <div class="form-group">
                                                <textarea type="Email" class="form-control" hresize
                                                          placeholder="Yogyakarta">
                                                </textarea>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </main>
  </div>
</div>
<%= render("components/footer") %>


